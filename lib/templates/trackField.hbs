<TrackFieldRequest USERID="{{userID}}">{{#if revision}}
  <Revision>{{revision}}</Revision>{{/if}}{{#if clientIP}}
  <ClientIp>{{clientIP}}</ClientIp>{{/if}}{{#each trackID}}
  <TrackID ID="{{this.id}}">{{#if this.destinationZipCode}}
    <DestinationZipCode>{{this.destinationZipCode}}</DestinationZipCode>{{/if}}{{#if this.mailingDate}}
    <MailingDate>{{this.mailingDate}}</MailingDate>{{/if}}
  </TrackID>{{/each}}
</TrackFieldRequest>